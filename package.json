{
  "name": "nba-scoreboard",
  "version": "0.0.6",
  "description": "nba scoreboard",
  "scripts": {
    "test": "npm run build && mocha --compilers js:babel-core/register",
    "coverage": "babel-node ./node_modules/istanbul/lib/cli cover node_modules/.bin/_mocha",
    "coveralls": "npm run-script coverage && node_modules/.bin/coveralls < coverage/lcov.info",
    "build": "babel src --presets babel-preset-es2015 --out-dir build",
    "prepublish": "npm run build"
  },
  "author": "Jae Bradley",
  "license": "MIT",
  "preferGlobal": true,
  "bin": {
    "nba": "build/cli/CommandLineClient.js"
  },
  "dependencies": {
    "commander": "2.9.0",
    "cli-table2": "0.2.0",
    "jstimezonedetect": "1.0.6",
    "moment-timezone": "0.5.4",
    "colors": "1.1.2 ",
    "node-emoji": "1.3.0 ",
    "nba-images": "0.1.2",
    "request-promise": "3.0.0",
    "q": "1.4.1"
  },
  "devDependencies": {
    "chai": "3.5.0",
    "mocha": "2.4.5",
    "coveralls": "2.11.9",
    "mocha-lcov-reporter": "1.2.0 ",
    "istanbul": "0.4.3 ",
    "babel-core": "6.8.0",
    "babel-preset-es2015": "6.6.0",
    "babel-node-debug": "2.0.0",
    "babel-cli": "6.7.7",
    "babel-preset-stage-0": "6.5.0",
    "babel-register": "6.8.0",
    "babel-polyfill": "6.8.0"
  },
  "directories": {
    "test": "test"
  },
  "repository": {
     "type": "git",
     "url": "https://github.com/jaebradley/nba-cli/tree/master"
   }
}